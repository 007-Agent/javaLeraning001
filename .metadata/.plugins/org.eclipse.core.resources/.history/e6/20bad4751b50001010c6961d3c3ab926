package lessonJava;

public class lesson24 {
	public static void main(String[] args) {
		 Human h1 = new Human("Bob", 12);
		 System.out.print(h1);
	}
}

class Human{
	
	private String name;
	private int age;
	
	
	public Human(String name, int age) {
		this.name = name;
		this.age = age;
		
	}
	
	
	
	
}