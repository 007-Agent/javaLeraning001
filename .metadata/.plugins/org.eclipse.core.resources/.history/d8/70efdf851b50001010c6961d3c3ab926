package lessonJava;

public class lesson24 {
	public static void main(String[] args) {
		 Humann h1 = new Humann("Bob", 12);
		 System.out.print(h1);
	}
}

class Humann{
	
	private String name;
	private int age;
	
	
	public Humann(String name, int age) {
		this.name = name;
		this.age = age;
		
	}
	
	
	
	
}